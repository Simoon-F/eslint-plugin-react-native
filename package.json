{
  "name": "@sj-distributor/eslint-plugin-react-native",
  "version": "0.3.0",
  "description": "ESLint presets for react native",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "main": "./lib/index.js",
  "exports": "./lib/index.js",
  "scripts": {
    "lint": "eslint .",
    "release": "release-it --no-increment",
    "test": "mocha tests --recursive"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-eslint-plugin": "^6.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-native": "^4.1.0",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-unicorn": "^52.0.0",
    "requireindex": "^1.2.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.1.0",
    "@release-it/conventional-changelog": "^5.1.1",
    "eslint": "^8.56.0",
    "mocha": "^10.0.0",
    "prettier": "^3.1.1",
    "release-it": "^15.5.0",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": "^18.18.0 || ^20.9.0 || >=21.1.0"
  },
  "peerDependencies": {
    "eslint": ">=8"
  },
  "license": "MIT",
  "repository": "https://github.com/sj-distributor/eslint-plugin-react-native",
  "bugs": {
    "url": "https://github.com/sj-distributor/eslint-plugin-react-native/issues"
  },
  "homepage": "https://github.com/sj-distributor/eslint-plugin-react-native#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
